{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "buildCommand": "NODE_OPTIONS='--openssl-legacy-provider' npm run build"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/:match*",
      "destination": "https://ku-gou-music-api-coral.vercel.app/:match*"
    }
  ]
}
